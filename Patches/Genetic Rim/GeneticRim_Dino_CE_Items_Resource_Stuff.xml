<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
		<li>GeneticRim Dinosauria Patch</li>
		</mods>

			<match Class="PatchOperationSequence">
				<operations>

			<!--DinoFeatherWool -->


			<li Class="PatchOperationReplace">
				<xpath>/Defs/ThingDef[defName="GR_DinoFeatherWool"]/statBases/StuffPower_Armor_Blunt</xpath>
				<value>
					<StuffPower_Armor_Blunt>0.24</StuffPower_Armor_Blunt>
				</value>
			</li>


			<!--GR_ScalyWool -->

			<li Class="PatchOperationReplace">
				<xpath>/Defs/ThingDef[defName="GR_ScalyWool"]/statBases/StuffPower_Armor_Sharp</xpath>
				<value>
					<StuffPower_Armor_Sharp>0.24</StuffPower_Armor_Sharp>
				</value>
			</li>

			<li Class="PatchOperationAdd">
				<xpath>/Defs/ThingDef[defName="GR_ScalyWool"]/statBases</xpath>
				<value>
					<StuffPower_Armor_Blunt>0.1</StuffPower_Armor_Blunt>
				</value>
			</li>



			</operations>
		</match>
	</Operation>
</Patch>